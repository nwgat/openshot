<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-export-option>
<export-option>
	<type translatable="True">nwgat.ninja</type>
	<title translatable="True">Nwgat MKV (x264/Opus)</title>
	<videoformat>mkv</videoformat>
	<videocodec>libx264</videocodec>
	<audiocodec>libopus</audiocodec>
	<audiochannels>2</audiochannels>
	<videobitrate
	 low="6 Mb/s"
	 med="12 Mb/s"
	 high="24.00 Mb/s"></videobitrate>
	<audiobitrate
	 low="96 kb/s"
	 med="169 kb/s"
	 high="256 kb/s"></audiobitrate>
	<samplerate>48000</samplerate>
</export-option>
